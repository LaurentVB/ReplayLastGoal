    "reload": "./node_modules/pm2/bin/pm2 reload server && ./node_modules/pm2/bin/pm2 reload twitter"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/xdamman/ReplayLastGoal.git"
  },
