{
  "name": "ReplayLastGoal",
  "version": "0.0.4",
  "description": "Automatically creates and tweet a video with the latest goal at the world cup",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/pm2/bin/pm2 start processes.json",
