  },
  "homepage": "https://github.com/xdamman/ReplayLastGoal",
  "dependencies": {
    "express": "^4.4.3",
    "CBuffer": "^0.1.4",
    "avconv": "^2.0.0",
    "underscore": "^1.6.0",
    "humanize": "0.0.9",
    "express3-handlebars": "^0.5.0",
    "pm2": "^0.8.15",
    "twitter": "^0.2.9",
    "request": "^2.36.0",
    "goo.gl": "^0.1.0"
  },
  "devDependencies": {
    "chai": "^1.9.1"
